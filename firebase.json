{
  "functions": {
    "source": "functions"
  },
  "hosting": [
    {
      "site": "mss-live-v2",
      "public": "public",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/api/**",
          "function": "api"
        },
        {
          "source": "**",
          "function": "api"
        }
      ]
    }
  ]
}